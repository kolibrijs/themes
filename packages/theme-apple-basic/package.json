{
  "name": "@kolibry/theme-apple-basic",
  "version": "0.0.1",
  "description": "Apple like theme for Kolibri",
  "author": "Dennis Ollhoff <contact@nyxb.email>",
  "license": "MIT",
  "homepage": "https://kolibri.show",
  "repository": {
    "type": "git",
    "url": "https://github.com/kolibry-js/themes"
  },
  "bugs": "https://github.com/kolibry-js/themes/issues",
  "keywords": [
    "kolibry-theme",
    "kolibry"
  ],
  "engines": {
    "node": ">=14.0.0",
    "kolibry": ">=0.0.1"
  },
  "kolibry": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "Helvetica Neue",
        "local": "Helvetica Neue"
      }
    }
  },
  "scripts": {
    "dev": "kolibry example.md --open",
    "build": "kolibry build example.md",
    "export": "kolibry export example.md",
    "screenshot": "kolibry export example.md --format png"
  },
  "dependencies": {
    "@kolibrijs/types": "^0.0.2",
    "codemirror-theme-vars": "^0.1.1",
    "prism-theme-vars": "^0.2.2",
    "theme-lumos": "^0.0.3"
  }
}
