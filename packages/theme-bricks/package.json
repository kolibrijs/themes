{
  "name": "@kolibrijs/theme-bricks",
  "version": "0.0.0",
  "description": "Bricks theme for Kolibri",
  "author": "Dennis Ollhoff <contact@nyxb.email>",
  "license": "MIT",
  "homepage": "https://kolibri.show",
  "repository": {
    "type": "git",
    "url": "https://github.com/kolibrijs/themes"
  },
  "bugs": "https://github.com/kolibrijs/themes/issues",
  "keywords": [
    "kolibri-theme",
    "kolibri"
  ],
  "engines": {
    "node": ">=14.0.0",
    "kolibri": ">=0.0.1"
  },
  "kolibri": {
    "colorSchema": "light",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "Signika Negative",
        "serif": "Sigmar One",
        "mono": "Fira Code"
      }
    }
  },
  "dependencies": {
    "@kolibrijs/types": "^0.0.1",
    "codemirror-theme-vars": "^0.1.1",
    "prism-theme-vars": "^0.2.2",
    "theme-lumos": "^0.0.3"
  }
}
