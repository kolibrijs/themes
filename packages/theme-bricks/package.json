{
  "name": "@kolibry/theme-bricks",
  "version": "0.0.0",
  "description": "Bricks theme for Kolibry",
  "author": "Dennis Ollhoff <contact@nyxb.email>",
  "license": "MIT",
  "homepage": "https://kolibry.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/kolibry-js/themes"
  },
  "bugs": "https://github.com/kolibri-js/themes/issues",
