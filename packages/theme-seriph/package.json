{
  "name": "@kolibry/theme-seriph",
  "version": "0.0.0",
  "description": "Seriph theme for Kolibry",
  "author": "Dennis Ollhoff <contact@nyxb.email>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/nyxb",
  "homepage": "https://kolibry.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/kolibry-js/themes"
  },
  "bugs": "https://github.com/kolibry-js/themes/issues",
  "keywords": [
    "kolibry-theme",
    "kokolibrylibri"
  ],
  "engines": {
    "node": ">=14.0.0",
    "kolibry": ">=0.0.1"
  },
  "kolibry": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "mono": "PT Mono",
        "sans": "PT Serif",
        "serif": "PT Serif",
        "italic": true,
        "weights": "400, 700"
      }
    }
  },
  "dependencies": {
    "@kolibrijs/types": "^0.0.2",
    "codemirror-theme-vars": "^0.1.1",
    "prism-theme-vars": "^0.2.2",
    "theme-lumos": "^0.0.3"
  }
}
